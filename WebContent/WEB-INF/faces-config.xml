<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<navigation-rule>
		<from-view-id>*</from-view-id>
			<navigation-case>
			      <from-outcome>successLoginAdmin</from-outcome>
			      <to-view-id>/views/accesoAdmin.xhtml</to-view-id>
			      <redirect/>
			</navigation-case> 
			<navigation-case>
			      <from-outcome>successLoginUser</from-outcome>
			      <to-view-id>/views/accesoUser.xhtml</to-view-id>
			      <redirect/>
			</navigation-case> 
			<navigation-case>
			      <from-outcome>inicioAdmin</from-outcome>
			      <to-view-id>/views/accesoAdmin.xhtml</to-view-id>
			      <redirect/>
			</navigation-case> 
			<navigation-case>
			      <from-outcome>inicioUser</from-outcome>
			      <to-view-id>/views/accesoUser.xhtml</to-view-id>
			      <redirect/>
			</navigation-case>
		    <navigation-case>
			    <from-outcome>failLogin</from-outcome>
			    <to-view-id>/views/principal.xhtml</to-view-id>
			    <redirect/>
			</navigation-case> 	
			<navigation-case>
				   <from-outcome>registerFail</from-outcome>
				   <to-view-id>/views/registro.xhtml</to-view-id>
			</navigation-case>
			<navigation-case>
				   <from-outcome>registerSuccess</from-outcome>
				   <to-view-id>/views/principal.xhtml</to-view-id>
			</navigation-case>
			<navigation-case>
				   <from-outcome>SessionClose</from-outcome>
				   <to-view-id>/views/principal.xhtml</to-view-id>
			</navigation-case>
			<navigation-case>
				   <from-outcome>PasswordView</from-outcome>
				   <to-view-id>/views/passwordView.xhtml</to-view-id>
			</navigation-case>
			<navigation-case>
				   <from-outcome>Table</from-outcome>
				   <to-view-id>/views/TableUsers.xhtml</to-view-id>
			</navigation-case>
			<navigation-case>
				   <from-outcome>TableBiciletas</from-outcome>
				   <to-view-id>/views/TableBicicletas.xhtml</to-view-id>
			</navigation-case>
			<navigation-case>
				   <from-outcome>altaBici</from-outcome>
				   <to-view-id>/views/registroBicicletas.xhtml</to-view-id>
			</navigation-case>
			<navigation-case>
				   <from-outcome>TableEstaciones</from-outcome>
				   <to-view-id>/views/TableEstaciones.xhtml</to-view-id>
			</navigation-case>
			<navigation-case>
				   <from-outcome>altaEstacion</from-outcome>
				   <to-view-id>/views/registroEstacion.xhtml</to-view-id>
			</navigation-case>
			<navigation-case>
				   <from-outcome>BicisUser</from-outcome>
				   <to-view-id>/views/TableBicisActuales.xhtml</to-view-id>
			</navigation-case>
			<navigation-case>
				   <from-outcome>historico</from-outcome>
				   <to-view-id>/views/TableHistorico.xhtml</to-view-id>
			</navigation-case>
			<navigation-case>
				   <from-outcome>loader</from-outcome>
				   <to-view-id>/views/loader.xhtml</to-view-id>
			</navigation-case>
	</navigation-rule>
</faces-config>
